---
layout: base.njk
---

<article class="h-entry">
    <ul>
        {% for tag in tags %}
        <li>
            {{tag}}
        </li>
        {% endfor %}
    </ul>

  <div class="e-content p-name">
    {{ content | safe }}
  </div>
  <a class="u-url" href="{{ page.url }}">
    <time class="dt-published" datetime="{{ date | htmlDateString }}">
      {{ date | readableDate }}
    </time>
  </a>
</article>

{% include "webmentions.njk" %}
